{
    "name": "blade-ui-kit/blade-zondicons",
    "description": "A package to easily make use of Zondicons in your Laravel Blade views.",
    "keywords": ["Blade", "Zondicons", "Laravel"],
    "homepage": "https://github.com/blade-ui-kit/blade-zondicons",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/blade-ui-kit/blade-zondicons/issues",
        "source": "https://github.com/blade-ui-kit/blade-zondicons"
    },
    "authors": [
        {
            "name": "Dries Vints",
            "homepage": "https://driesvints.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "blade-ui-kit/blade-icons": "^1.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BladeUI\\Zondicons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BladeUI\\Zondicons\\BladeZondiconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
